FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    location    "0";
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (4e-08 -0 -0 4e-08 -0 4e-08);

boundaryField
{
    bottomWall
    {
        type            calculated;
        value           uniform (4e-08 -0 -0 4e-08 -0 4e-08);
    }
    defaultFaces
    {
        type            empty;
    }
    topWall
    {
        type            calculated;
        value           uniform (4e-08 0 -0 4e-08 -0 4e-08);
    }
    outlet
    {
        type            cyclic;
    }
    inlet
    {
        type            cyclic;
    }
}


